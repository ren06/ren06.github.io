{
    "id": "com.synvance.basicwidget",
    "version": "0.1.3",
    "name": "Basic Widget",
    "description": "A basic Widget that sets a value",
    "newInstancePrefix": "BasicWidget",
    "icon" : "https://aydenowl.github.io/literate-enigma/icon.png",
    "vendor": "Synvance",
    "eula": "EULA",
    "license": "1.0",
    "webcomponents": [
        {
            "kind": "main",
            "tag": "com-synvance-basicwidget",
            "url": "https://ren06.github.io/basicWidget/basicWidget.js",
            "integrity": "",
            "ignoreIntegrity": true
        }
    ],
    "properties": {
        "value": {
            "type": "string",
            "description": "value",
            "default": ""
        }         
    },  
    "methods": { 
        "setValue": {           
            "description": "Set value",
            "parameters": [
                {
                    "name": "val",
                    "type": "string",
                    "description": "value"
                }
            ],
            "body": "this.value = val;"
        },
        "getValue": {
            "returnType": "string",
            "description": "Get the value",
            "body": "return this.value;"
        }        
    },
    "events": {
        "onClick": {
            "description": "Called when the user clicks the button."        
        }        
    }           
}